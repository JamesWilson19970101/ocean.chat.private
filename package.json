{
  "name": "ocean.chat",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:account": "NODE_ENV=development nest start account --watch",
    "start:ocean.chat": "NODE_ENV=development nest start ocean.chat --watch",
    "start:authorization": "NODE_ENV=development nest start authorization --watch",
    "start:stream": "NODE_ENV=development nest start stream --watch",
    "start:oceanchat-auth": "NODE_ENV=development nest start oceanchat-auth --watch",
    "start:dev": "concurrently \"yarn start:account\" \"yarn start:ocean.chat\" \"yarn start:authorization\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/ocean.chat/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:app/ocean.chat": "jest --config ./apps/ocean.chat/test/jest-e2e.json",
    "test:app/account": "jest --config ./jest.account.json",
    "test:lib/models": "jest --config ./libs//models/jest.config.json",
    "generatedoc": "typedoc"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-ws": "^11.1.6",
    "@nestjs/websockets": "^11.1.6",
    "@ocean.chat/casbin-ioredis-adapter": "^1.0.0",
    "argon2": "^0.41.1",
    "casbin": "^5.38.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "i18next": "^24.2.3",
    "ioredis": "5.3.2",
    "joi": "^17.13.3",
    "mongodb": "^6.17.0",
    "mongoose": "^8.13.1",
    "nats": "^2.29.3",
    "nestjs-pino": "^4.3.1",
    "opossum": "^9.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/hbs": "^4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/opossum": "^8.1.9",
    "@types/passport": "^0",
    "@types/passport-jwt": "^4",
    "@types/passport-local": "^1",
    "@types/supertest": "^6.0.2",
    "@types/ws": "8.18.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "2.6.2",
    "typedoc": "^0.28.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@ocean.chat/common-exceptions(|/.*)$": "<rootDir>/libs/common-exceptions/src/$1",
      "^@ocean.chat/i18n(|/.*)$": "<rootDir>/libs/i18n/src/$1",
      "^@ocean.chat/models(|/.*)$": "<rootDir>/libs/models/src/$1",
      "^@ocean.chat/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@ocean.chat/settings(|/.*)$": "<rootDir>/libs/settings/src/$1"
    }
  },
  "packageManager": "yarn@4.7.0"
}

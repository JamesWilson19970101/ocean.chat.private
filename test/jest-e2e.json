{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "ocean/i18n": "<rootDir>/../libs/i18n/src",
    "ocean/i18n/(.*)": "<rootDir>/../libs/i18n/src/$1",
    "ocean/logger": "<rootDir>/../libs/logger/src",
    "ocean/logger/(.*)": "<rootDir>/../libs/logger/src/$1"
  }
}